{
  "name": "@codesgate/realtor",
  "version": "0.1.5-alpha.3",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "nodemon --watch src --ext ts --exec 'node --loader ts-node/esm src/index.ts'",
    "check-types": "tsc --noEmit",
    "migrate": "npx prisma migrate dev",
    "db:seed": "ts-node src/prisma/seed.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "../core/dist"
    ],
    "ext": "ts,json",
    "exec": "node --enable-source-maps --loader ts-node/esm src/index.ts"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "nodemon": "^3.1.9"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "ts-node": "10.9.2",
    "tsup": "8.3.5"
  }
}